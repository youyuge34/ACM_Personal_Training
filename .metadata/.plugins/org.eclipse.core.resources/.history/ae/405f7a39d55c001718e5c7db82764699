import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner in = new Scanner(System.in);
		while(in.hasNext()){
			int n = in.nextInt();
			int[] list = new int[n];
			for(int i=0;i<n;i++){
				list[i] = in.nextInt();
			}
			
		}
	}

}
